{
  "name": "hitvictor",
  "version": "1.0.0",
  "description": "一個有趣的網頁遊戲，讓你發洩對 Victor 的不滿！",
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "web": "vite",
    "build:web": "vite build",
    "build:android": "cd android && ./gradlew assembleRelease",
    "build:ios": "cd ios && xcodebuild -workspace HitVictor.xcworkspace -scheme HitVictor -configuration Release",
    "clean": "cd android && ./gradlew clean && cd ../ios && xcodebuild clean"
  },
  "dependencies": {
    "@react-native-firebase/app": "^18.7.3",
    "@react-native-firebase/firestore": "^18.7.3",
    "firebase": "^10.8.0",
    "react": "18.2.0",
    "react-native": "^0.73.4",
    "react-native-gesture-handler": "^2.15.0",
    "react-native-haptic-feedback": "^2.2.0",
    "react-native-reanimated": "^3.6.2",
    "react-native-safe-area-context": "^4.9.0",
    "react-native-sound": "^0.11.2"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/babel-preset": "0.73.21",
    "@react-native/eslint-config": "0.73.2",
    "@react-native/metro-config": "0.73.5",
    "@react-native/typescript-config": "0.73.1",
    "@types/react": "^18.2.6",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.6.3",
    "eslint": "^8.19.0",
    "gh-pages": "^6.3.0",
    "jest": "^29.6.3",
    "prettier": "2.8.8",
    "react-test-renderer": "18.2.0",
    "typescript": "5.0.4",
    "vite": "^5.1.4"
  },
  "engines": {
    "node": ">=18"
  }
}
